config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd33:facd:0cdc::/48'

config interface 'lan'
	option type 'bridge'
	option ifname 'eth0'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ip6assign '60'

config interface 'wan'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd33:facd:0cdc::/48'

config interface 'lan'
	option type 'bridge'
	option ifname 'eth0'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ip6assign '60'

config interface 'wan'
	option ifname 'eth1'
	option _orig_ifname 'eth1'
	option _orig_bridge 'false'
	option proto 'pppoe'
	option username '2633554'
	option password '2633554'
	option defaultroute '0'
	option peerdns '0'

config interface 'vpn'
	option ifname 'vpn'
	option proto 'pptp'
	option username 'fttb_advancea'
	option password '12345678a'
	option server '94.158.50.1'
	option pppd_options 'nomppe'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option ports '1 2 3 4 0'

config 'route' 'pptp_server'
        option 'interface' 'wan'
        option 'target' '94.158.50.1'
        option 'netmask' '255.255.255.255'


config zone                                     
        option name 'comnet'                    
        list subnet '172.16.236.0/24'           
        option input 'ACCEPT'                   
        option output 'ACCEPT'                  
        option forward 'ACCEPT'



opkg install nf_conntrack_pptp



